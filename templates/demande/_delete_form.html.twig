<form method="post" action="{{ path('demande_delete', {'id': demande.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">

    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ demande.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
